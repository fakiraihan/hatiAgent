# Environment Configuration for Hati Project

# Groq API Key - dapatkan dari https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama3-70b-8192

# Spotify API - dapatkan dari https://developer.spotify.com/
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Google Maps API - dapatkan dari https://console.cloud.google.com/ (OPSIONAL - butuh billing)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Foursquare API - dapatkan dari https://developer.foursquare.com/ (OPSIONAL - sering error)
FOURSQUARE_API_KEY=your_foursquare_api_key_here

# OpenStreetMap/Nominatim - GRATIS! Tidak perlu API key, sudah terintegrasi
# Digunakan sebagai sumber utama untuk pencarian tempat di Indonesia

# GOAPI - untuk pencarian tempat di Indonesia (OPSIONAL - tidak aktif)
GOAPI_KEY=your_goapi_key_here

# Giphy API - dapatkan dari https://developers.giphy.com/
GIPHY_API_KEY=your_giphy_api_key_here

# TMDb API - dapatkan dari https://www.themoviedb.org/settings/api
TMDB_API_KEY=your_tmdb_api_key_here

# Application Settings
APP_HOST=0.0.0.0
APP_PORT=8000
DEBUG=true

# CORS Settings
FRONTEND_URL=http://localhost:8080

# Logging
LOG_LEVEL=INFO
